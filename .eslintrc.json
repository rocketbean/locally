{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard-with-typescript",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "require-jsdoc": "off",
        "linebreak-style": ["error"],
        "space-infix-ops": ["error"],
        "no-undef": ["error"],
        "arrow-spacing": ["error"],
        "no-useless-return": "error",
        "no-extra-semi": "error",
    }
}
